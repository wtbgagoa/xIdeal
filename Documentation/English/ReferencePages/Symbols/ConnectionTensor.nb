      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"epsilonmetric", "[", 
        RowBox[{
         RowBox[{"-", "a"}], ",", 
         RowBox[{"-", "b"}], ",", "i", ",", "j"}], "]"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"e2", "[", 
           RowBox[{"-", "i"}], "]"}], 
          RowBox[{"e3", "[", 
           RowBox[{"-", "j"}], "]"}]}], "-", 
         RowBox[{
          RowBox[{"e2", "[", 
           RowBox[{"-", "j"}], "]"}], 
          RowBox[{"e3", "[", 
           RowBox[{"-", "i"}], "]"}]}]}], ")"}]}], ",", 
      RowBox[{"{", 
